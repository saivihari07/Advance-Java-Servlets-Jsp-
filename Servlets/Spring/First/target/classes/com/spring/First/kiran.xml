<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean name="emp" class="com.spring.First.Employee">
		<constructor-arg value="09"></constructor-arg>
		<constructor-arg value="Kiran"></constructor-arg>
	</bean>

	<bean name="address" class="com.spring.First.Address">
		<constructor-arg value="USA colony"></constructor-arg>
		<constructor-arg value="Hyderabad"></constructor-arg>
		<constructor-arg value="Telangana"></constructor-arg>
		<constructor-arg value="India"></constructor-arg>
	</bean>

	 <bean name="emp2" parent="emp">
		<constructor-arg ref="address"></constructor-arg>
	</bean> 


</beans>

